 Nmet[2] <- NemuAmet
 Nmet[3] <- NmueAmet
 Nmet[4] <- NmumuAmet
 Nmet[5] <- NeeBmet
 Nmet[6] <- NemuBmet
 Nmet[7] <- NmueBmet
 Nmet[8] <- NmumuBmet


 NeeBmetTemp   <- NeeBmet
 NmumuBmetTemp <- NmumuBmet

 NeeABmetTemp   <- NeeBmet + NeeAmet
 NmumuABmetTemp <- NmumuBmet + NmumuAmet

}

